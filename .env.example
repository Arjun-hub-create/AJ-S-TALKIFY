# Environment Variables Template
# Copy this file and create .env in the server folder

# MongoDB Atlas Connection String
MONGODB_URI=mongodb+srv://your_username:your_password@cluster.mongodb.net/your_database?retryWrites=true&w=majority

# JWT Secret Key (Generate a random string)
JWT_SECRET=your-super-secret-jwt-key-here-change-this

# Port (Railway will set this automatically)
PORT=3000

# Node Environment
NODE_ENV=production
